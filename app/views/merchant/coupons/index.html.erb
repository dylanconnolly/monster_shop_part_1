<%= link_to "Create New Coupon", new_merchant_coupon_path %>

<% @coupons.each do |coupon| %>
  <section id="coupon-<%= coupon.id %>">
    <%= coupon.name %>
    <%= coupon.code %>
    <%= coupon.percent_off %>
    <%= link_to "Edit Coupon", edit_merchant_coupon_path(coupon) %>
  </section>
<% end %>
